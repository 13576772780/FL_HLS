do
		python main_fedrep.py --alg fedavg --epochs 100 --num_users 20 --shard_per_user 5 --limit_local_output 0 --local_rep_ep 3 --local_only 1 --is_reset_dataset 0 --is_concept_shift 1 --frac 0.3 --dataset cifar10 --model cnn --moment 0.5 --is_reset_model 1;

